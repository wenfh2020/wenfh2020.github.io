<span class="kt">void</span> <span class="nf">test_align_mod</span><span class="p">()</span> <span class="p">{</span>
    <span class="kt">char</span> <span class="n">bin</span><span class="p">[</span><span class="mi">128</span><span class="p">];</span>
    <span class="n">u_char</span> <span class="o">*</span><span class="n">p</span><span class="p">,</span> <span class="o">*</span><span class="n">a</span><span class="p">,</span> <span class="o">*</span><span class="n">r</span><span class="p">;</span>
